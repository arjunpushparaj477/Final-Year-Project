<script>
function contactFormValids(){

	$filter    = /^([a-z0-9_\.\-])+\@(([a-z0-9\-])+\.)+([a-z0-9]{2,4})+$/;
	$uname     = $('#cuname').val();
	$email     = $('#ctxtEmailid').val();
	$contact   = $('#ccontacts').val();
	$mesg      = $('#cmesg').val();


    if($uname=="") { 
		document.getElementById('cerrors').innerHTML="Name must be filled out";
        return false;
    }
    else{
        document.getElementById('cerrors').innerHTML="";
        
    }
	
	if($email=="") {
		 document.getElementById('ceid').innerHTML="Please enter email-id";
		 return false;
    }else{
         document.getElementById('ceid').innerHTML="";
		
    }
	
    if(!$filter.test($email)) {
		document.getElementById('ceid').innerHTML="Please enter valid email-id";
		return false;
	}else{
	    document.getElementById('ceid').innerHTML="";
	}
	if ($contact=="" || $contact=="Null") {
		document.getElementById('ccon').innerHTML="Contact number must be filled out";
        return false;
    }else{
        document.getElementById('ccon').innerHTML="";
    }
	if(isNaN($contact)||$contact.indexOf(" ")!=-1)
	{
	  document.getElementById('ccon').innerHTML="Contact number must be numeric value";
	  return false; 
	}else{
	    document.getElementById('ccon').innerHTML="";
	}
	
	if(($contact.length < 10) || ($contact.length > 10)){
		document.getElementById('ccon').innerHTML="Enter 10 digit valid Mobile Number";
		return false;
	}else{
	    document.getElementById('ccon').innerHTML="";
	}
	if($mesg=="" || $mesg=="Null") {
		document.getElementById('cmsg').innerHTML="Message must be filled out";
        return false;
    }else{
        document.getElementById('cmsg').innerHTML="";
    }
    $.ajax({
		type: "POST",
		url: "https://www.ant-pc.com/home/contact",
		data:{uname:$uname,email:$email,contact:$contact,mesg:$mesg},
		success: function(res){
		   /* $('#status').html(res); */
            $('#qconfigform')[0].reset();
		}
	});
}

</script>


