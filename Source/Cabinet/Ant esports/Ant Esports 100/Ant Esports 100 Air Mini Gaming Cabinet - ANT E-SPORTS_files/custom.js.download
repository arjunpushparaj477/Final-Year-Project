$(document).ready(function(){

  $('.stars a').append('<i class="fas fa-star"></i>');

  $(".stars a").click(function(){
    $clicked = this.className;
    $clickedRating = $clicked.charAt($clicked.length - 1);
    $('.stars a i').css("color", "#ccc"); 
    $('.stars a.star-'+$clickedRating+' i').css("color", "yellow");
    $('.stars a.star-'+($clickedRating-1)+' i').css("color", "yellow");
    $('.stars a.star-'+($clickedRating-2)+' i').css("color", "yellow");
    $('.stars a.star-'+($clickedRating-3)+' i').css("color", "yellow");
    $('.stars a.star-'+($clickedRating-4)+' i').css("color", "yellow");
    $('.stars a.star-'+($clickedRating-5)+' i').css("color", "yellow");
  });

});



 $(document).ready(function(){

